{
    "name": "asprega/breadcrumb-bundle",
    "description": "A bundle for generating dynamic breadcrumbs in Symfony applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [ "Breadcrumb", "Breadcrumbs", "Symfony", "Bundle" ],
    "authors": [
        {
            "name": "Andrea Sprega",
            "email": "andreasprega@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "doctrine/common": "~2.3",
        "symfony/translation-contracts": "^1.0|^2.0",
        "symfony/framework-bundle": "^4.0|^5.0",
        "symfony/property-access": "^4.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.20",
        "mockery/mockery": "1.3.1"
    },
    "autoload": {
        "psr-4": { "AndreaSprega\\Bundle\\BreadcrumbBundle\\": "" }
    },
    "archive": {
        "exclude": [ "/Tests" ]
    }
}
