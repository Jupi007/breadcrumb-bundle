{
    "name": "asprega/breadcrumb-bundle",
    "description": "A bundle for generating dynamic breadcrumbs in Symfony applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "Breadcrumb",
        "Breadcrumbs",
        "Symfony",
        "Bundle"
    ],
    "authors": [
        {
            "name": "Andrea Sprega",
            "email": "andreasprega@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "doctrine/annotations": "^1.0",
        "symfony/framework-bundle": "^4.0|^5.0",
        "symfony/property-access": "^4.0|^5.0",
        "symfony/translation-contracts": "^1.0|^2.0",
        "twig/twig": "^2.10|^3.0"
    },
    "require-dev": {
        "mockery/mockery": "1.3.1",
        "phpunit/phpunit": "9.4.4"
    },
    "autoload": {
        "psr-4": {
            "AndreaSprega\\Bundle\\BreadcrumbBundle\\": ""
        }
    },
    "archive": {
        "exclude": [
            "/Tests"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
